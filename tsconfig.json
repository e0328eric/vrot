{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "rootDir": "./src",
    "baseUrl": "./",
    "paths": {
      "@pkg/*": ["vrot-web/pkg/*"]
    },
    "outDir": "vrot-web",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
